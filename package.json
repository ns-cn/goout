{
  "name": "goout-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "build": "cd frontend && npm run build",
    "start": "node server.js",
    "test": "jest",
    "vercel-build": "npm run build",
    "frontend": "cd frontend && npm run serve",
    "backend": "nodemon server.js",
    "dev:all": "concurrently \"npm run backend\" \"cd frontend && npm run serve\"",
    "install:all": "npm install && cd frontend && npm install",
    "serve:prod": "npm run build && http-server frontend/dist -p 8080"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "goout-backend": "file:",
    "http-server": "^14.1.1",
    "mongodb": "^6.10.0"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.7"
  }
}
